{
  "name": "bytic/application",
  "license": "MIT",
  "description": "bytic Framework - application package",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Application\\": "src/"
    },
    "classmap": [
      "legacy"
    ],
    "files": [
      "src/functions/application.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Application\\Tests\\": "tests/",
      "Nip\\Application\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.1",
    "bytic/autoloader": "^0.9",
    "bytic/config": "^0.9",
    "bytic/database": "^0.9",
    "bytic/debug": "^0.9",
    "bytic/debugbar": "^0.9",
    "bytic/dispatcher": "^0.9",
    "bytic/filesystem": "^0.9",
    "bytic/http": "^0.9",
    "bytic/locale": "^0.9",
    "bytic/mail": "^0.9",
    "bytic/mvc": "^0.9",
    "bytic/router": "^0.9",
    "bytic/staging": "^0.9",
    "bytic/translation": "^0.9",
    "filp/whoops": "^2.3",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5|^7.2",
    "mockery/mockery": "^1.0"
  }
}
