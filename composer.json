{
  "name": "bytic/application",
  "license": "MIT",
  "description": "bytic Framework - application package",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\Application\\": "src/"
    },
    "classmap": [
      "legacy"
    ],
